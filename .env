# Configuration de base
NODE_ENV=development
PORT=5000

# Base de données
DATABASE_PATH=./server/database/jacques_ia.db

# JWT
JWT_SECRET=votre_jwt_secret_super_securise_changez_moi_en_production
JWT_EXPIRES_IN=7d

# Google OAuth (temporairement désactivé)
GOOGLE_CLIENT_ID=disabled
GOOGLE_CLIENT_SECRET=disabled
REACT_APP_GOOGLE_CLIENT_ID=disabled

# APIs IA (optionnelles - l'app fonctionne sans)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# Configuration CORS
CORS_ORIGIN=http://localhost:3000

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
RATE_LIMIT_AI_MAX_REQUESTS=20
RATE_LIMIT_CODE_MAX_REQUESTS=10

# Logging
LOG_LEVEL=info